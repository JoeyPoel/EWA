{
  "name": "auc-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "buildprod": "vue-cli-service build --mode production",
    "builddev": "vue-cli-service build --mode development",
    "start": "npx http-server -p 9000 ./dist",
    "lint": "vue-cli-service lint",
    "test": "jest",
    "check versions": "npm outdated",
    "update": "npm update --save",
    "audit": "npm audit fix"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.17.0",
    "@mdi/js": "^7.3.67",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.1",
    "chart.js": "^4.4.0",
    "core-js": "^3.8.3",
    "fetch-intercept": "^2.4.0",
    "http-server": "^14.1.1",
    "jwt-decode": "^4.0.0",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.2.0",
    "vue-popperjs": "^2.3.0",
    "vue-router": "4.1.3",
    "vuetify": "^3.4.3",
    "webpack-plugin-vuetify": "^2.0.1",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/eslint-parser": "^7.12.16",
    "@mdi/font": "^7.3.67",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/vue3-jest": "^27.0.0",
    "eslint": "^8.54.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-vue": "^9.18.1",
    "jest": "^27.5.1",
    "jest-expect-message": "^1.1.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "plugins": [
      "jest"
    ],
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ],
    "testMatch": [
      "**/src/**/*.spec.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "/images/(.*)$": "<rootDir>/public/images/$1",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "@vue/vue3-jest",
      "^.+\\.(jpg|jpeg|png|gif|svg|mp3|mp4|mpeg)$": "<rootDir>/jest-media-transformer.js"
    },
    "verbose": false
  }
}
